{
  "build": {
    "beforeBuildCommand": "yarn && yarn build",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:3001",
    "distDir": "../out",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Seoder",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "writeFile": true
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "seoder",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.seoder.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https:/seoder.io/api/update/{{target}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU0RjMzNUNEMkRBREQ4NzQKUldSMDJLMHR6VFh6NU40aXVqZmxXbDJ0VkYzRExJekcxVHVsQWxNRXJvU0d6M3A4OElaYWJCeXYK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Seoder",
        "width": 920,
        "minWidth": 920,
        "minHeight": 600
      }
    ]
  }
}
